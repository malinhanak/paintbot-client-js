{
  "name": "paintbot-client-js",
  "version": "0.1.0",
  "author": "Max Davidson <max.davidson@cygni.se>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "build": "run-p build:*",
    "build:lib": "rimraf lib && tsc",
    "build:es": "rimraf es && tsc --module es2015 --outDir es",
    "lint": "tslint --project tsconfig.json --format stylish --exclude '**/*.json'",
    "format": "prettier --write '**/*.{md,yml,js,json,ts,tsx}'",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "es",
    "lib"
  ],
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6"
  }
}
